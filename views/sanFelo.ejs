<%- include('partials/_header') %>

<div class="d-flex justify-content-center">
  <div class="d-flex justify-content-between col-6">
    <h3 >Bodega en Venta, San Felipe</h3>
    <h3>Precio</h3>
  </div>
</div>

<div class="d-flex justify-content-center">
  <div class="d-flex justify-content-between col-6">
    <p>Sector Suroccidente, Barranquilla Área 147.6 m2</p>
    <p>$ 280.000.000</p>
  </div>
</div>


<div class="d-flex justify-content-center mt-2">
  <div id="carouselExample" class="carousel slide col-6">
    <div class="carousel-inner">
      <div class="carousel-item active">
        <img src="/images/1.jpg" class="d-block w-100" alt="pic1">
      </div>
      <div class="carousel-item">
        <img src="/images/2.jpg" class="d-block w-100" alt="pic2">
      </div>
      <div class="carousel-item">
        <img src="/images/3.jpg" class="d-block w-100" alt="pic3">
      </div>
      <div class="carousel-item">
        <img src="/images/4.jpg" class="d-block w-100" alt="pic4">
      </div>
      <div class="carousel-item">
        <img src="/images/5.jpg" class="d-block w-100" alt="pic5">
      </div>
      <div class="carousel-item">
        <img src="/images/6.jpg" class="d-block w-100" alt="pic6">
      </div>
      <div class="carousel-item">
        <img src="/images/7.jpg" class="d-block w-100" alt="pic7">
      </div>
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExample" data-bs-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#carouselExample" data-bs-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Next</span>
    </button>
